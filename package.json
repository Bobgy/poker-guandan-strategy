{
  "name": "poker-guandan-strategy",
  "version": "1.0.0",
  "description": "guandan strategy calculation",
  "main": "lib/strategy.wasm",
  "scripts": {
    "npm:init:win": "npm config set script-shell \"C:\\bin\\Git\\bin\\bash.exe\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run clean && mkdir -p docs/static",
    "build": "em++ strategy.cpp -o docs/static/strategy.js -s EXTRA_EXPORTED_RUNTIME_METHODS='[\"cwrap\", \"ccall\"]' --bind",
    "build:prod": "npm run build -- -O2 && cd guandan-app && npm run build && cp -r build ../docs/static",
    "clean": "rm -rf a.out* a.exe docs/static/*",
    "aa-deploy": "cd guandan-app && npm run build",
    "deploy": "rm -rf docs/* && cp -r guandan-app/build/* docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bobgy/poker-guandan-strategy.git"
  },
  "keywords": [
    "guandan",
    "strategy",
    "wasm"
  ],
  "author": "Yuan Gong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bobgy/poker-guandan-strategy/issues"
  },
  "homepage": "https://github.com/Bobgy/poker-guandan-strategy#readme",
  "devDependencies": {}
}